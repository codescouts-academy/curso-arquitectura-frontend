{
  "compilerOptions": {
    "module": "commonjs",
    "declaration": false,
    "strict": true,
    "strictPropertyInitialization": false,
    "noImplicitAny": true,
    "removeComments": true,
    "downlevelIteration": true,
    "sourceMap": true,
    "esModuleInterop": true,
    "target": "es5",
    "jsx": "react",
    "lib": ["es6", "dom"],
    "baseUrl": "./",
    "types": ["jest"],
    "experimentalDecorators": true,
    "emitDecoratorMetadata": true
  },
  "exclude": ["node_modules", "lib"]
}
